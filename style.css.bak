@charset"UTF-8";

/* CSS Document */
 body {
    font: 17px/1.4 Verdana, Arial, Helvetica, sans-serif;
    background: #F0F0F0;
    margin: 0;
    padding: 0;
}
a img {
    border:none;
}
a{
	text-decoration:none;
	}
a:hover{
	text-decoration:underline;
	}
* {
    margin:0;
    padding:0;
}
.overlay{
	position:fixed;
	width:100%;
	height:100%;
	z-index:500;
	background:#FFF;
	opacity:0.9;
 -ms-filter:"alpha(opacity=0.5)";
  filter: alpha(opacity=0.5);
  -webkit-transition-property: z-index, opacity;
  transition-property: z-index, opacity;
  -webkit-transition: 1s linear;
  transition: 1s linear;
	}
.message{
	position:fixed;
	top: 50%;
  left: 50%;
	width:500px;
	height:400px;
	background-color:rgb(253, 253, 253);
  margin-top: -155px; 
  margin-left: -250px; 
  text-align:center;
  border: 1px rgb(168, 168, 168) solid;
	border-radius: 11px 11px 11px 11px;
	}
.message p{
	text-align:left;
	text-indent:20px;
	margin: 0 auto;
	width: 450px;
	}
.aligner{
	margin:0 auto;
	width:450px;
	}
.formOverlay{
	text-align:left;
	
	}
.inputSurround{
	width: 375px;
	height: 26px;
	margin: 0 auto;
}
.subitbox{
	}
input[type=submit]{
	height: 22px;
	float: right;
	margin-top: 2px;
}
.spacing{
	margin:10px 0 10px 0;
	}
.textarea{
	width:100%;
	resize:none;
	display:block;
	}
.container{
	width:1500px;
	margin:0 auto;
	position:relative;
	}
.surround{
	width:700px;
	margin:0 auto;
	text-align:center;
	}
.formNorm{
	text-align:left;
	}
.dataType{
	margin-top: 25px;
	background-color: rgb(214, 214, 214);
	border: 1px solid rgb(177, 172, 172);
	width: 700px;
	border-spacing: 0px;
	}
.dataMode a{
	color:black;
	text-decoration:none;
	width: 100%;
	display: block;
	}
.selected{
	background-color: rgb(243, 243, 243);
	}
#graph{
		margin-top: 5px;
		border-left: 1px black solid;
		border-bottom: 1px black solid;
	}
.graphTitle{
		margin-top:25px;
		width:700px;
		font-size:18px;
		height:20px;
		font-weight: bold;
}
.Labels{
		z-index:100;
		position:absolute;
		text-align:center;
	}
.vertical{
		-webkit-transform:rotate(270deg);
		-moz-transform: rotate(270deg);
		-ms-transform: rotate(270deg);
		-o-transform: rotate(270deg);
		filter: progid:DXImageTransform.Microsoft.BasicImage(rotation=270);
	}
.selectType{
    text-align: left;
}
.importMore{
	text-align:left;
	}